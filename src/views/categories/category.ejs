<h1>category: <%= category.vehicle_category_name %></h1>

<div class="grid">
    <% category.forEach((vehicle) => { %>
        <div class="card">
            <a href="/vehicle/<%= vehicle.vehicle_id %>" class="vehicle-card-link">
            <img src="<%= vehicle.vehicle_image %>" alt="<%= vehicle.vehicle_name %>">
            <h3><%= vehicle.vehicle_name %></h3>
            <p><%= vehicle.vehicle_price %></p>
            <p><%= vehicle.vehicle_description %></p>
            </a>
        </div>
    <% }); %>
</div>